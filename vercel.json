
{
  "version": 2,
  "rewrites": [
    {
      "source": "/watch",
      "destination": "/server.js"
    },
    {
      "source": "/watch/",
      "destination": "/server.js"
    },
    {
      "source": "/watch(.*)",
      "destination": "/server.js"
    }
  ],
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ]
}
